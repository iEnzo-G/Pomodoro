platform :ios, '16.0'

target 'Pomodoro' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  pod 'Firebase'
  pod 'FirebaseCore'
  pod 'Firebase/Analytics'

  # Pods for Pomodoro

  target 'PomodoroTests' do
    inherit! :search_paths
    # Pods for testing
  pod 'Firebase'
  pod 'FirebaseCore'
  pod 'Firebase/Analytics'
  end

end
